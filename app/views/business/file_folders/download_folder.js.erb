$('.loading').removeClass('hide');
$('.download-folder').addClass('hide');
    var requestloop = setInterval(function () {
        $.ajax({
            type: 'GET',
            url: '/business/file_folders/'+ "<%= params[:id] %>" +'/check_zip',
            format: 'json',
            success: function (data) {
                if (data.complete != false) {
                    $('.loading').addClass('hide');
                    $('.download-folder').removeClass('hide');
                    clearInterval(requestloop);
                    $('#js-zip-download').modal('show');
                    $('.download-zip').attr("href", "<%= download_folder_business_file_folder_path(params[:id]) %>")
                }
            }
        });
    }, 3000)

